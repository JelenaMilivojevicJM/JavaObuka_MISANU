package zadatak1;

public class Genericka<T>{

	T atribut;
	
	Genericka(T atribut){
		this.atribut = atribut;
	}

	public T getAtribut() {
		return atribut;
	}

	public void setAtribut(T atribut) {
		this.atribut = atribut;
	}
	
	public void prikaziTip() {
		System.out.println(atribut+" ima tip vrednosti: "+atribut.getClass().getName());
	}
}
