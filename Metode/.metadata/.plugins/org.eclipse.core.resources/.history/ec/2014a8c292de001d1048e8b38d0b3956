package jelena.milivojevic;
import java.text.DecimalFormat;
public class HornerovaSema {
	static double horn(double[] a, int n, double x) {
	    double horner = a[n];
	    while (n != 0) {
	        horner = horner * x + a[n - 1];
	        n--;
	    }

	    return horner;
	    }
	public static void main(String[] args) {
		/*
		 * Napisati program za tabeliranje vrednosti funkcije f(x) = x4 - 10x3 + 35x2 -
		 * 50x + 24. Koristeći Hornerovu šemu za izračunavanje vrednosti funkcije. Funkciju
		 * tabelirati za vrednost x = 0.0(0.1)0.9.
		 */

	}

}
