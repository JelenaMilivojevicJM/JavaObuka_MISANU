package jelena.milivojevic;

public class LogickaPromenljiva {

	public static void main(String[] args) {
		/*Napisati kod koji prikazuje promenjive logiƒçkog tipa. */
		
		//Inicijalizacija i Deklaracija promenljivih logicnog tipa
		boolean tacno = true;
		boolean netacno = false;
		//Stampanje
		System.out.println(tacno);
		System.out.println(netacno);
	}

}
