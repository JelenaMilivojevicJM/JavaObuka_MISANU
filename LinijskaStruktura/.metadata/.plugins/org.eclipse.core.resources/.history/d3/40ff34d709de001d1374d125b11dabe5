package jelena.milivojevic;

public class Main {

	public static void main(String[] args) {
		// Napisati program za štampanje
		
		//Za stampanje ovde koristimo System.out.print ili System.out.println
		
		//Stampanje primera zaglavlja:
		System.out.print("\tR.br.\tX\tY\tZ\n");
		
		//Stampanje primera vrednosti
		System.out.println("\t1\t2.5\t3.6\t4.9");
		
		/* Izlaz: Obim kruga je 31.415901 za poluprečnik 5.0 */
	}

}
