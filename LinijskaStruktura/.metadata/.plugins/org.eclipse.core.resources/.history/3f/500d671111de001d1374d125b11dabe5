package jelena.milivojevic;

public class DeklaracijaPromenljive {

	public static void main(String[] args) {
		/*Deklarisati celobrojnu promenljivu x i realne promenljive a, b i c tipa double. 
		 * Promenljivu b inicijalizovati sa 3,45, a c sa 56,78. */
		
		int x;
		double a, b, c;
		b = 3.45d;
		c = 56.78d;
	}

}
